{
  "name": "deneth-fashion-admin",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://deneth-fashion-backend.vercel.app/api",
    "VITE_ADMIN_KEY": "supersecretadminkey123"
  }
}